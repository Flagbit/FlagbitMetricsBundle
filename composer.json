{
    "name": "flagbit/metrics-bundle",
    "description": "FlagbitMetricsBundle provides easy integration of metrics collector services for the Symfony Framework",
    "type": "symfony-bundle",
    "keywords": ["metrics", "logging"],
    "homepage": "https://github.com/Flagbit/FlagbitMetricsBundle",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "David Fuhr",
            "email": "david.fuhr@flagbit.de"
        },
        {
            "name": "Antonio Mansilla",
            "email": "antonio.mansilla@flagbit.de"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "beberlei/metrics": "~1.0",
        "symfony/framework-bundle": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "conflict": {
        "symfony/dependency-injection": ">=2.6.0"
    },
    "autoload": {
        "psr-0": {
            "Flagbit\\Bundle\\MetricsBundle": ""
        }
    },
    "target-dir": "Flagbit/Bundle/MetricsBundle"
}
